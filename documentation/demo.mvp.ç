
@define const char 	*OSX = "OSX";
// need to register a label without additional parsing
@define platform const char 	*platform = OSX;
@variable name: int = 4;

@preprocess	platform check
{
	if (platform != OSX )
		exit(_(Unsupported platform.));
}
@undefine OSX
@undefine platform

#include <stdio.h>
//soon : @include <stdio.h>
//soon : optional interoperabiloty with C-Pre-Processor

int main()
{
	printf("%s\n", @preprocess { _(42); });
}


/*
*	Primitives :
		@define
		@undefine

		@define prototype
		@undefine prototype

		@preprocess


	Std :

		@decorate
		@include file.c
		@parser name
		@compiler name
		@macro name

	Language :

		import / export
		lambda
		async/await
		class
		deferer
		....


*
